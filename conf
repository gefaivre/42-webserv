server {
    
    listen  8080;
    server_name domain1.com;
    cgi .php /usr/bin/php-cgi;
    root /mnt/nfs/homes/jbach/Documents/WebservBeforeCGI/www;
    location /a/ {
      root /mnt/nfs/homes/jbach/Documents/WebservBeforeCGI/www;
      # server_name domain1.com;
    }
    autoindex on;
    client_max_body_size 8000000000000;
    server_name domain2.com ;
    accepted_methods       GET POST DELETE;
    location /abc/ {
      root /mnt/nfs/homes/jbach/Documents/WebservBeforeCGI/php;
    }
  }
  server {
    listen  9090;
    autoindex on;
    server_name domain1.com;
